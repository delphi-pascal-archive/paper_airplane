<?xml version="1.0" standalone="yes"?>
<airplane>
	<constants>
		<constant>
			<name>a</name>
			<value>100</value>
		</constant>
		<constant>
			<name>b</name>
			<value>70</value>
		</constant>
		<constant>
			<name>c</name>
			<value>25</value>
		</constant>
	</constants>
	<basepoints>
		<point>
			<x>c</x>
			<y>0</y>
		</point>
		<point>
			<x>b + c</x>
			<y>0</y>
		</point>
		<point>
			<x>0</x>
			<y>c</y>
		</point>
		<point>
			<x>c</x>
			<y>c</y>
		</point>
		<point>
			<x>b + c</x>
			<y>c</y>
		</point>
		<point>
			<x>b + (2 * c)</x>
			<y>c</y>
		</point>
		<point>
			<x>0</x>
			<y>a + c</y>
		</point>
		<point>
			<x>c</x>
			<y>a + c</y>
		</point>
		<point>
			<x>b + c</x>
			<y>a + c</y>
		</point>
		<point>
			<x>b + (2 * c)</x>
			<y>a + c</y>
		</point>
		<point>
			<x>c</x>
			<y>a + (2 * c)</y>
		</point>
		<point>
			<x>b + c</x>
			<y>a + (2 * c)</y>
		</point>
		<point>
			<x>c</x>
			<y>2 * (a + c)</y>
		</point>
		<point>
			<x>b + c</x>
			<y>2 * (a + c)</y>
		</point>
	</basepoints>
	<sheet>
		<point>1</point>
		<point>4</point>
		<point>3</point>
		<point>7</point>
		<point>8</point>
		<point>11</point>
		<point>13</point>
		<point>14</point>
		<point>12</point>
		<point>9</point>
		<point>10</point>
		<point>6</point>
		<point>5</point>
		<point>2</point>
	</sheet>
	<actions>
		<action>
			<p1>8</p1>
			<p2>9</p2>
			<targets>
				<target>1</target>
			</targets>
			<angle>pi / 2</angle>
		</action>
		<action>
			<p1>4</p1>
			<p2>8</p2>
			<targets>
				<target></target>
				<target>1</target>
			</targets>
			<angle>pi / 2</angle>
		</action>
		<action>
			<p1>9</p1>
			<p2>5</p2>
			<targets>
				<target></target>
				<target>3</target>
			</targets>
			<angle>pi / 2</angle>
		</action>
		<action>
			<p1>5</p1>
			<p2>4</p2>
			<targets>
				<target></target>
				<target>4</target>
			</targets>
			<angle>pi / 2</angle>
		</action>
		<action>
			<p1>11</p1>
			<p2>12</p2>
			<targets>
				<target></target>
				<target>2</target>
			</targets>
			<angle>pi / 2</angle>
		</action>
	</actions>
</airplane>
