<?xml version="1.0" standalone="yes"?>
<airplane>
	<constants>
		<constant>
			<name>width</name>
			<value>210</value>
		</constant>
		<constant>
			<name>height</name>
			<value>298</value>
		</constant>
		<constant>
			<name>w</name>
			<value>width</value>
		</constant>
		<constant>
			<name>h</name>
			<value>height</value>
		</constant>
		<constant>
			<name>alpha</name>
			<value>pi / 10</value>
		</constant>
		<constant>
			<name>h0</name>
			<value>w / 8</value>
		</constant>
	</constants>
	<basepoints>
		<point>
			<x>w / 2</x>
			<y>h</y>
		</point>
		<point>
			<x>0</x>
			<y>h - (w / 2)</y>
		</point>
		<point>
			<x>w</x>
			<y>h - (w / 2)</y>
		</point>
		<point>
			<x>w / 2</x>
			<y>h - (w / 2)</y>
		</point>
		<point>
			<x>w</x>
			<y>h - w</y>
		</point>
		<point>
			<x>0</x>
			<y>h - w</y>
		</point>
		<point>
			<x>(w / 2) + h0</x>
			<y>0</y>
		</point>
		<point>
			<x>(w / 2) - h0</x>
			<y>0</y>
		</point>
		<point>
			<x>0</x>
			<y>0</y>
		</point>
		<point>
			<x>w</x>
			<y>0</y>
		</point>
		<point>
			<x>w</x>
			<y>h</y>
		</point>
		<point>
			<x>0</x>
			<y>h</y>
		</point>
	</basepoints>
	<sheet>
		<point>9</point>
		<point>10</point>
		<point>11</point>
		<point>12</point>
	</sheet>
	<actions>
		<action>
			<p1>2</p1>
			<p2>1</p2>
			<targets>
				<target>1</target>
			</targets>
			<angle>pi</angle>
		</action>
		<action>
			<p1>1</p1>
			<p2>3</p2>
			<targets>
				<target>1</target>
			</targets>
			<angle>pi</angle>
		</action>
		<action>
			<p1>2</p1>
			<p2>3</p2>
			<targets>
				<target>2</target>
				<target>1</target>
				<target>3</target>
			</targets>
			<angle>pi</angle>
		</action>
		<action>
			<p1>4</p1>
			<p2>5</p2>
			<targets>
				<target>4</target>
				<target>3</target>
				<target>1</target>
			</targets>
			<angle>pi</angle>
		</action>
		<action>
			<p1>6</p1>
			<p2>4</p2>
			<targets>
				<target>1</target>
				<target>7</target>
				<target>2</target>
			</targets>
			<angle>pi</angle>
		</action>
		<action>
			<p1>4</p1>
			<p2>1</p2>
			<targets>
				<target>7</target>
				<target>1</target>
				<target>5</target>
				<target>4</target>
				<target>3</target>
				<target>6</target>
			</targets>
			<angle>(pi / 2) - alpha</angle>
		</action>
		<action>
			<p1>1</p1>
			<p2>4</p2>
			<targets>
				<target>1</target>
				<target>8</target>
				<target>10</target>
				<target>9</target>
				<target>2</target>
				<target>12</target>
			</targets>
			<angle>(pi / 2) - alpha</angle>
		</action>
		<action>
			<p1>4</p1>
			<p2>7</p2>
			<targets>
				<target>3</target>
				<target>4</target>
				<target>5</target>
				<target>7</target>
				<target>6</target>
				<target>11</target>
			</targets>
			<angle>alpha - (pi / 2)</angle>
		</action>
		<action>
			<p1>8</p1>
			<p2>4</p2>
			<targets>
				<target>1</target>
				<target>12</target>
				<target>8</target>
				<target>10</target>
				<target>2</target>
				<target>9</target>
			</targets>
			<angle>alpha - (pi / 2)</angle>
		</action>
	</actions>
</airplane>
